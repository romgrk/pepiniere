    "immutable": "^3.8.2",
    "is-object": "^1.0.1",
    "localforage": "^1.9.0",
    "match-sorter": "^2.2.0",
    "node-sass": "^4.14.1",
    "node-sass-chokidar": "0.0.3",
    "object-assign": "4.1.1",
    "object-equals": "^0.3.0",
